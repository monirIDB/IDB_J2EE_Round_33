<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <servlet>
        <servlet-name>Alamin</servlet-name>
        <servlet-class>com.test.TestServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Alamin</servlet-name>
        <url-pattern>/Test</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Alamin1</servlet-name>
        <servlet-class>com.test.TestServletNew</servlet-class>
        <init-param>
            <param-name>version</param-name>
            <param-value>Second version</param-value>
                
        </init-param>
    </servlet>
    <servlet-mapping>
        <servlet-name>Alamin1</servlet-name>
        <url-pattern>/Test1</url-pattern>
    </servlet-mapping>
    <context-param>
        <param-name>name</param-name>     
        <param-value> Your name is Rahim</param-value>
    </context-param>
    <context-param>
        <param-name>email</param-name>      
        <param-value> Your Email is rahim@gmail.com</param-value>
    </context-param>
     
    
</web-app>
